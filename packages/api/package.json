{
  "name": "@lssm/package.api",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "clean": "rimraf dist",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\""
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "dependencies": {
    "@nestjs/mapped-types": "*"
  },
  "devDependencies": {
    "@lssm/package.typescript-config": "*",
    "@types/node": "^20.3.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
